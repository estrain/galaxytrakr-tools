<?xml version="1.0"?>
<data_managers>
  <data_manager
      tool_file="data_manager/data_manager_amrfinderplus.xml"
      id="data_manager_amrfinderplus"
      tool_data_table_conf="tool_data_table_conf.xml.sample">

    <data_table name="amrfinderplus_versioned_database">
      <output>
        <column name="value" />
        <column name="name" />
        <column name="path" output_ref="output_file">
          <move type="directory" relativize_symlinks="True">
            <source>${path}</source>
            <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">
              amrfinderplus-db/${value}
            </target>
          </move>
          <value_translation>
            ${GALAXY_DATA_MANAGER_DATA_PATH}/amrfinderplus-db/${value}
          </value_translation>
          <value_translation type="function">abspath</value_translation>
        </column>
      </output>
    </data_table>
  </data_manager>
</data_managers>

